# http://editorconfig.org

root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
indent_style = space
indent_size = 4
line_length=120
trim_trailing_whitespace = true

[*.py]
known_first_party=ace
multi_line_output=3
default_section=THIRDPARTY

[*.md]
trim_trailing_whitespace = false

[*.scss]
indent_size = 2

[Makefile]
indent_style = tab

[nginx.conf]
indent_style = space
indent_size = 2
